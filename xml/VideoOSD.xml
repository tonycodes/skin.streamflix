<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">500</defaultcontrol>
    <controls>
        <control type="group">
            <visible>Player.ShowInfo</visible>
            <posx>0</posx>
            <posy>0</posy>
            <control type="image">
                <posx>0</posx>
                <posy>0</posy>
                <width>1920</width>
                <height>150</height>
                <texture colordiffuse="DD141414">white.png</texture>
            </control>
            <control type="label">
                <posx>60</posx>
                <posy>30</posy>
                <width>1800</width>
                <height>45</height>
                <font>font_title</font>
                <textcolor>FFFFFFFF</textcolor>
                <label>$INFO[Player.Title]</label>
            </control>
            <control type="label">
                <posx>60</posx>
                <posy>85</posy>
                <width>1800</width>
                <height>35</height>
                <font>font13</font>
                <textcolor>FF808080</textcolor>
                <label>$INFO[Player.Time] / $INFO[Player.Duration]</label>
            </control>
        </control>
        <control type="group">
            <posx>0</posx>
            <posy>880</posy>
            <control type="image">
                <posx>0</posx>
                <posy>0</posy>
                <width>1920</width>
                <height>200</height>
                <texture colordiffuse="DD141414">white.png</texture>
            </control>
            <control type="progress">
                <posx>60</posx>
                <posy>30</posy>
                <width>1800</width>
                <height>8</height>
                <texturebg colordiffuse="FF333333">white.png</texturebg>
                <lefttexture colordiffuse="FFE50914">white.png</lefttexture>
                <midtexture colordiffuse="FFE50914">white.png</midtexture>
                <righttexture colordiffuse="FFE50914">white.png</righttexture>
                <overlaytexture>-</overlaytexture>
                <info>Player.Progress</info>
            </control>
            <control type="grouplist" id="500">
                <posx>660</posx>
                <posy>60</posy>
                <width>600</width>
                <height>80</height>
                <orientation>horizontal</orientation>
                <itemgap>20</itemgap>
                <align>center</align>
                <control type="button" id="501">
                    <width>80</width>
                    <height>80</height>
                    <font>font16</font>
                    <align>center</align>
                    <label>|&lt;</label>
                    <onclick>PlayerControl(Previous)</onclick>
                </control>
                <control type="button" id="502">
                    <width>80</width>
                    <height>80</height>
                    <font>font16</font>
                    <align>center</align>
                    <label>&lt;&lt;</label>
                    <onclick>PlayerControl(Rewind)</onclick>
                </control>
                <control type="button" id="503">
                    <width>100</width>
                    <height>80</height>
                    <font>font20_bold</font>
                    <align>center</align>
                    <label>$INFO[Player.Paused,||,>]</label>
                    <onclick>PlayerControl(Play)</onclick>
                </control>
                <control type="button" id="504">
                    <width>80</width>
                    <height>80</height>
                    <font>font16</font>
                    <align>center</align>
                    <label>>></label>
                    <onclick>PlayerControl(Forward)</onclick>
                </control>
                <control type="button" id="505">
                    <width>80</width>
                    <height>80</height>
                    <font>font16</font>
                    <align>center</align>
                    <label>>|</label>
                    <onclick>PlayerControl(Next)</onclick>
                </control>
            </control>
            <control type="label">
                <posx>60</posx>
                <posy>150</posy>
                <width>400</width>
                <height>35</height>
                <font>font13</font>
                <textcolor>FF808080</textcolor>
                <label>$INFO[Player.Time]</label>
            </control>
            <control type="label">
                <posx>1460</posx>
                <posy>150</posy>
                <width>400</width>
                <height>35</height>
                <font>font13</font>
                <textcolor>FF808080</textcolor>
                <align>right</align>
                <label>$INFO[Player.Duration]</label>
            </control>
        </control>
    </controls>
</window>
